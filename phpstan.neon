includes:
    - vendor/phpstan/phpstan-strict-rules/rules.neon
parameters:
    level: 8
    paths:
        - src
        - tests
    excludePaths:
        - %rootDir%/../../../tests/temp/*
    treatPhpDocTypesAsCertain: false
    ignoreErrors:
        -
            identifier: method.unused
        -
            identifier: missingType.iterableValue
        -
            identifier: missingType.generics
        -
            identifier: varTag.nativeType
