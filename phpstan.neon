parameters:
    level: 8
    paths:
        - src
        - tests
    excludePaths:
        - %rootDir%/../../../tests/temp/*
    treatPhpDocTypesAsCertain: false
    ignoreErrors:
        -
            identifier: method.unused
        -
            identifier: missingType.iterableValue
        -
            identifier: missingType.generics
        - '#Access to an uninitialized readonly property MyTester\\Tester::\$folder#'
