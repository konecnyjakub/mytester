{
    "name": "konecnyjakub/mytester",
    "description": "Simple tester for PHP",
    "license": "BSD-3-Clause",
    "keywords": [
      "php", "testing", "tester", "test", "simple", "command  line", "assertions"
    ],
    "authors": [
        {
          "name": "Jakub Konečný",
          "email": "jakub.konecny2@centrum.cz",
          "role": "Developer"
        }
    ],
    "support": {
        "sources": "https://gitlab.com/konecnyjakub/mytester",
        "issues": "https://gitlab.com/konecnyjakub/mytester/-/issues"
   },
    "require": {
        "php": ">=8.1.0",
        "composer-runtime-api": "^2.0",
        "ayesh/php-timer": "^2.2",
        "nette/utils": "^3.2.10",
        "nette/finder": "^2.6",
        "nette/command-line": "^1.7"
    },
    "require-dev": {
        "nette/robot-loader": "^3.4.2",
        "nette/di": "^3.1",
        "nette/bootstrap": "^3.2.1",
        "php-parallel-lint/php-parallel-lint": "^1.3.2",
        "php-parallel-lint/php-console-highlighter": "^0.5",
        "squizlabs/php_codesniffer": "^3.7.2",
        "phpstan/phpstan": "^1.10.50",
        "phpunit/php-code-coverage": "^9.2.30"
    },
    "conflict": {
        "nette/robot-loader": "<3.4.2",
        "nette/di": "<3.1"
    },
    "autoload": {
        "psr-4": {
            "MyTester\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyTester\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "bin": [
        "src/mytester"
    ]
}
