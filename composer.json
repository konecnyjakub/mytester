{
    "name": "konecnyjakub/mytester",
    "description": "Simple tester for PHP",
    "license": "BSD-3-Clause",
    "keywords": [
      "php", "testing", "tester", "test", "simple", "command  line", "browser"
    ],
    "authors": [
        {
          "name": "Jakub Konečný",
          "email": "jakub.konecny2@centrum.cz",
          "role": "Developer"
        }  
    ],
    "support": {
        "sources": "https://github.com/konecnyjakub/mytester/",
        "issues": "https://github.com/konecnyjakub/mytester/issues"
   },
    "require": {
        "php": ">=7.1.0",
        "nette/reflection": "^2.4",
        "tracy/tracy": "^2.4"
    },
    "require-dev": {
        "nette/robot-loader": "^3.0",
        "nette/di": "^2.4",
        "nette/bootstrap": "^2.4",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "squizlabs/php_codesniffer": "^3.0",
        "phpstan/phpstan": "^0.7",
        "phpstan/phpstan-nette": "^0.7"
    },
    "suggest": {
        "nette/robot-loader": "Needed for automated tests runner."
    },
    "autoload": {
        "psr-4": {
            "MyTester\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "bin": [
        "src/mytester"
    ]
}
