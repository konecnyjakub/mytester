{
    "name": "konecnyjakub/mystester",
    "description": "Simple tester for PHP",
    "license": "BSD-3-Clause",
    "keywords": [
      "php", "testing", "tester", "test", "simple", "command  line", "browser"
    ],
    "authors": [
        {
          "name": "Jakub Konečný",
          "email": "jakub.konecny2@centrum.cz",
          "role": "Developer"
        }  
    ],
    "support": {
        "issues": "https://gitlab.com/konecnyjakub/mytester/issues"
   },
    "require": {
        "php": ">=5.6.0",
        "nette/reflection": "~2.4",
        "tracy/tracy": "~2.4"
    },
    "require-dev": {
        "nette/robot-loader": "~2.4"
    },
    "suggest": {
        "nette/robot-loader": "Needed for automated tests runner."
    },
    "autoload": {
        "psr-4": {
            "MyTester\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
